{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "stylelint src/**/*.{css,scss} --fix",
    "lint": "npm run eslint; npm run check-types; npm run prettier; npm run stylelint",
    "lint:fix": "npm run eslint:fix; npm run check-types; npm run prettier:fix; npm run stylelint:fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.2",
    "vite": "^2.4.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.ts": "bash -c tsc --noEmit",
    "*.{js,ts,json,md}": "prettier --write",
    "*.{css,scss}": "stylelint --fix"
  },
  "dependencies": {
    "@k-kind/svg-generator": "0.0.4"
  }
}
